{
    "name": "koeker/laravel-discord-notification",
    "description": "A Laravel package for sending notifications to Discord channels via webhooks.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "KOeker"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": ">=8.0",
        "guzzlehttp/guzzle": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Koeker\\LaravelDiscordNotification\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Koeker\\LaravelDiscordNotification\\DiscordWebhookServiceProvider"
            ],
            "aliases": {
                "DiscordWebhook": "LaravelDiscordNotification"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
